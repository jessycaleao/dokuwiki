[virtual]
user = www-data
group = www-data
listen = 127.0.0.1:9000
pm = dynamic
pm.max_children = 100
pm.start_servers = 30
pm.min_spare_servers = 20
pm.max_spare_servers = 70
pm.max_requests = 10000
pm.process_idle_timeout = 3600s
pm.max_requests = 100
request_terminate_timeout = 1800s
catch_workers_output = yes
chdir = /